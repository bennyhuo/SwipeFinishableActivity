<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bennyhuo.swipefinishable.sample" >

    <application
        android:name="com.bennyhuo.swipefinishable.sample.DemoApplication"
        android:allowBackup="true"
        android:configChanges="orientation"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.bennyhuo.swipefinishable.sample.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.bennyhuo.swipefinishable.sample.DetailActivity" android:configChanges="orientation" android:theme="@style/AppTranslucentTheme"/>
    </application>

</manifest>